<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="CarparkList">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="carpark"/>
        <xs:element ref="validfrom"/>
        <xs:element ref="validtill"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="carpark">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="carpark_id"/>
        <xs:element ref="company"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="carpark_id" type="xs:string"/>
  <xs:element name="company" type="xs:string"/>
  <xs:element name="validfrom" type="xs:string"/>
  <xs:element name="validtill" type="xs:string"/>
</xs:schema>
