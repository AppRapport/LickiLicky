<xs:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           attributeFormDefault="unqualified"
           elementFormDefault="qualified"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="modified_order"
                minOccurs="0"
                maxOccurs="unbounded">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="item"
                            minOccurs="0"
                            maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="description"
                                        type="xs:string"
                                        minOccurs="0" />
                            <xs:element name="confirmed_quantity"
                                        type="xs:unsignedByte"
                                        minOccurs="0" />
                        </xs:sequence>
                        <xs:attribute name="id"
                                      type="xs:unsignedByte"
                                      use="optional" />
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="order_id"
                          type="xs:unsignedInt" />
        </xs:complexType>
    </xs:element>
</xs:schema>
